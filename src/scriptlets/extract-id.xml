<?xml version="1.0" encoding="UTF-8"?>
<scriptlet id="extractNodeId" xmlns="http://www.gitb.com/tdl/v1/">
  <namespaces>
    <ns prefix="sr">http://www.w3.org/2005/sparql-results#</ns>
  </namespaces>
  <imports>
    <artifact type="binary" encoding="UTF-8" name="query">resources/queries/extract-id.rq</artifact>
    <artifact type="binary" encoding="UTF-8" name="xpath">resources/queries/extract-id.xp</artifact>
  </imports>
  <params>
    <var name="treeNode" type="string" />
    <var name="contentType" type="string" />
  </params>
  <steps>
    <process handler="RdfUtils" operation="select" output="nodeIdResults">
      <input name="model">$treeNode</input>
      <input name="inputContentType">$contentType</input>
      <input name="query">$query</input>
    </process>
    <process handler="XPathProcessor" output="url">
      <input name="input">$nodeIdResults</input>
      <input name="expression">$xpath</input>
    </process>
  </steps>
  <output name="url"/>
</scriptlet>